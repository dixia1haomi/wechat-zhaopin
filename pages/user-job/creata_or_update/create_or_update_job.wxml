<view class="{{rootSheetState && 'page-noscroll'}}">
  <!-- <scroll-view class='asd'> -->

  <zy-hengxian margin="30" text="发布岗位"></zy-hengxian>

  <my-input bind:inputEvent="jobname_Event" text="岗位名称" zhanwei="输入岗位名称"></my-input>

  <my-cell catchtap="sheetState_pay" title="岗位薪资" text="{{pay_list[pay_value]}}" jiantou hover>
    <my-sheetdown state="{{sheetState_pay}}" bind:itemEvent="sheet_pay" list="{{pay_list}}" cancel title></my-sheetdown>
  </my-cell>

  <my-cell catchtap="welfareState" title="福利" text="{{welfareStr || '可多选'}}" jiantou hover>
    <my-sheetbox state="{{welfareState}}" bind:itemEvent="welfare" list="{{welfare_list}}" cancel title></my-sheetbox>
  </my-cell>

  <my-input bind:inputEvent="numberEvent" text="招聘人数" zhanwei="输入招聘人数" input_type="number"></my-input>

  <my-cell catchtap="sheetState_exp" title="经验要求" text="{{exp_list[exp_value]}}" jiantou hover>
    <my-sheetdown state="{{sheetState_exp}}" bind:itemEvent="sheet_exp" list="{{exp_list}}" cancel title></my-sheetdown>
  </my-cell>

  <my-cell catchtap="sheetState_education" title="学历要求" text="{{education_list[education_value]}}" jiantou hover>
    <my-sheetdown state="{{sheetState_education}}" bind:itemEvent="sheet_education" list="{{education_list}}" cancel title></my-sheetdown>
  </my-cell>


  <my-cell catchtap="asd" title="地址" text="前往发布" jiantou hover></my-cell>
  <my-cell catchtap="asd" title="地址名称" text="前往发布" jiantou hover></my-cell>
  <my-cell catchtap="asd" title="你的称呼" text="前往发布" jiantou hover></my-cell>
  <my-cell catchtap="asd" title="电话" text="前往发布" jiantou hover></my-cell>
  <my-cell catchtap="asd" title="岗位描述" text="前往发布" jiantou hover></my-cell>


  <my-cell catchtap="sheetTap" title="单选框框" text="{{sheetDownItem || '请选择' }}" jiantou hover>
    <my-sheetdown class="" state="{{sheetState}}" bind:itemEvent="itemE" cancel title></my-sheetdown>
  </my-cell>


  <my-cell catchtap="checkboxTap" title="多选" text="{{checkboxValueStr || '请选择' }}" jiantou hover>
    <my-sheetbox state="{{checkboxState}}" bind:itemEvent="checkboxitemE" cancel title></my-sheetbox>
  </my-cell>

</view>
